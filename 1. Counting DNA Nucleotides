# Nucleotide counter

nucleotides = ["A", "C", "G", "T"]

DNA_sample = "ACGT"

def nucleotide_count(seq):
    nucleotides_calculator = {"A": 0, "C": 0, "G": 0, "T": 0}
    for num in seq:
        nucleotides_calculator[num] += 1
    return nucleotides_calculator

print(nucleotide_count(DNA_sample))
